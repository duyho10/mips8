module mips8(D1,D0,S,Y);
input wire [7:0] D0, D1;
input wire S;
output reg [7:0] Y;

Mux2to1 d0(D1[0],D0[0],S,Y[0]);
Mux2to1 d1(D1[1],D0[1],S,Y[1]);
Mux2to1 d2(D1[2],D0[2],S,Y[2]);
Mux2to1 d3(D1[3],D0[3],S,Y[3]);
Mux2to1 d4(D1[4],D0[4],S,Y[4]);
Mux2to1 d5(D1[5],D0[5],S,Y[5]);
Mux2to1 d6(D1[6],D0[6],S,Y[6]);
Mux2to1 d7(D1[7],D0[7],S,Y[7]);

endmodule
